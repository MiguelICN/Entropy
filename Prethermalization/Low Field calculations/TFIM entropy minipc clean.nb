Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"::", "Section", "::"}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
     RowBox[{"2", ")"}], " ", "Import", " ", "for", " ", 
     RowBox[{"plotting", ":", 
      RowBox[{"Entropy", 
       RowBox[{"(", "t", ")"}], " ", "curves"}]}]}], "->", 
    RowBox[{"PNG", " ", "per", " ", "Entropy_", "*", 
     RowBox[{".", "mx"}]}]}], "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"!", 
       RowBox[{"ValueQ", "[", "pageEntropy", "]"}]}], ",", 
      RowBox[{
       RowBox[{"pageEntropy", "[", 
        RowBox[{"La_", ",", "Lb_"}], "]"}], ":=", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"La", "*", 
          RowBox[{"Log", "[", "2", "]"}]}], "-", "0.5"}], ")"}]}]}], "]"}], 
    ";"}], "\n", 
   RowBox[{
    RowBox[{"myGradient", "=", 
     RowBox[{
      RowBox[{"Blend", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"0", ",", "Blue"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"0.5", ",", "Purple"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"1", ",", "Red"}], "}"}]}], "}"}], ",", "#"}], "]"}], 
      "&"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"processAndPlotEntropy", "[", "fullPath_", "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "dataAssoc", ",", "L", ",", "g", ",", "hRangeName", ",", "tlistLocal",
          ",", "hValues", ",", "rawData", ",", "w", ",", "tSmoothed", ",", 
         "plotData", ",", "minLogH", ",", "maxLogH", ",", "colorList", ",", 
         "legend", ",", "yPage", ",", "plotTitle", ",", "outputDir", ",", 
         "pngName", ",", "finalPath", ",", "finalPlot", ",", "tMin", ",", 
         "tMax", ",", "sClean", ",", "entropyData"}], "}"}], ",", 
       RowBox[{
        RowBox[{
        "Print", 
         "[", "\"\<-------------------------------------------------\>\"", 
         "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{"\"\<Reading: \>\"", ",", 
          RowBox[{"FileBaseName", "[", "fullPath", "]"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"dataAssoc", "=", 
         RowBox[{"Import", "[", "fullPath", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"L", "=", 
         RowBox[{"dataAssoc", "[", "\"\<L\>\"", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"g", "=", 
         RowBox[{"dataAssoc", "[", "\"\<g\>\"", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"hRangeName", "=", 
         RowBox[{"dataAssoc", "[", "\"\<h_range_name\>\"", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"tlistLocal", "=", 
         RowBox[{"Flatten", "[", 
          RowBox[{"dataAssoc", "[", "\"\<tlist\>\"", "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"hValues", "=", 
         RowBox[{"dataAssoc", "[", "\"\<h_values\>\"", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"rawData", "=", 
         RowBox[{"dataAssoc", "[", "\"\<data\>\"", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"outputDir", "=", 
         RowBox[{"DirectoryName", "[", "fullPath", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"w", "=", "100"}], ";", "\[IndentingNewLine]", 
        RowBox[{"tSmoothed", "=", 
         RowBox[{"MovingAverage", "[", 
          RowBox[{"tlistLocal", ",", "w"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"tMin", "=", 
         RowBox[{"Min", "[", "tlistLocal", "]"}]}], ";", 
        RowBox[{"tMax", "=", 
         RowBox[{"Max", "[", "tlistLocal", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"plotData", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"entropyData", "=", 
             RowBox[{"entry", "[", 
              RowBox[{"[", "2", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"sClean", "=", 
             RowBox[{"MovingAverage", "[", 
              RowBox[{
               RowBox[{"Re", "@", 
                RowBox[{"Flatten", "@", "entropyData"}]}], ",", "w"}], 
              "]"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"If", "[", 

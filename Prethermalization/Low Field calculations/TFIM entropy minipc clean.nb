              RowBox[{"Transpose", "[", 
               RowBox[{"{", 
                RowBox[{"tSmoothed", ",", "sClean"}], "}"}], "]"}], ",", 
              RowBox[{"{", "}"}]}], "]"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"entry", ",", "rawData"}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"minLogH", "=", 
         RowBox[{"Min", "[", 
